{
  "name": "pathpilot-mimic-integration",
  "version": "1.0.0",
  "description": "PathPilot Lab Intelligence with MIMIC-IV FHIR Data",
  "scripts": {
    "dev": "concurrently --kill-others --names \"FHIR-API,PathPilot-UI\" -c \"bgBlue.bold,bgGreen.bold\" \"npm:dev:fhir\" \"npm:dev:ui\"",
    "dev:fhir": "cd backend && python3 oracle_fhir_api.py",
    "dev:ui": "cd pathpilot-demo && npm run dev",
    "start": "concurrently --kill-others --names \"FHIR-API,PathPilot-UI\" -c \"bgBlue.bold,bgGreen.bold\" \"npm:start:fhir\" \"npm:start:ui\"",
    "start:fhir": "cd backend && python3 oracle_fhir_api.py",
    "start:ui": "cd pathpilot-demo && npm start",
    "install:deps": "pip3 install -r backend/requirements.txt && cd pathpilot-demo && npm install"
  },
  "keywords": [
    "fhir",
    "pathology",
    "lab",
    "mimic",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
